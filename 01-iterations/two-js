
let index = 100
while ( index >= 10) {
    //console.log(`value of index is ${index}`);
    index = index - 3
}

let myArray = ["0", "1", "2", "3", "4", "5"]
let arr = 0
//console.log(myArray.length);
while (arr < myArray.length) {
    //console.log(`value is ${myArray[arr]}`);
    arr = arr + 1
}



let score = 20
do {
    console.log(`score is ${score}`);
    score = score + 1
 } while (score <= 30)
